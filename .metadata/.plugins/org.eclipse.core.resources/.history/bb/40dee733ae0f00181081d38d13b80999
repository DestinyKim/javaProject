package test.com;

public class Test01Student extends Test01person{
	int num;
	int kor;
	int eng;
	int math;
	int total = kor + eng + math;
	double avg = total / 3;
	String grade;
	
	//@선언:annotation (상속이 전제되어 있어야함.)슈퍼클래스에 있는 메소드를 재정의하여 사용함.
	//상속이 전제, 리턴타입과 매개변수타입 동일하게 재 정의.
	@Override
	public void ppp() {
		System.out.println("Student.ppp()");
		super.ppp();
	}
	
	public void sss() {
		
	}

}
